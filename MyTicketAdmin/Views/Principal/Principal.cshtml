
@{
    Layout = null;
    var a = "";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Principal</title>
    <link rel="stylesheet" href="~/Estilos/cssRegistro.css" />
</head>
<body>
   
    <div id="pos">
        <h1>Panel de control</h1>
    </div>
    <div id="cuerpoCentral" class="central">
        <aside id="menuLat">
            <h1><Strong>Menu</Strong></h1>
            
            <ul>
                <li> <a href="../Register/IngresoPersona" " target="ifInfo">Ingresar persona</a></li>
                <li>@Html.ActionLink("Registrar usuario","Registro","Register")</li>
                <li>Eliminacion usuario</li>
