@model MyTicketAdmin.Models.DatosObj.MPersona


@using (Html.BeginForm("guardarPersona","Register",FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <div>
        @Html.LabelFor(Model => Model.nombre,"Nombre")
        @Html.TextBoxFor(Model => Model.nombre)
        @Html.ValidationMessageFor(model => model.nombre)

        @Html.LabelFor(model => model.AppPaterno,"Primer Apellido")
        @Html.TextBoxFor(model => model.AppPaterno)
        @Html.ValidationMessageFor(model => model.AppPaterno)

        @Html.LabelFor(model => model.AppMaterno,"Segundo Apellido")
        @Html.TextBoxFor(model => model.AppMaterno)

        @Html.LabelFor(model => model.Rut,"Rut")
        @Html.TextBoxFor(model => model.Rut)
        @Html.ValidationMessageFor(model => model.Rut)

        @Html.LabelFor(model => model.DV,"Digito Verificador")
        @Html.TextBoxFor(model => model.DV)
        @Html.ValidationMessageFor(model => model.DV)

        @Html.LabelFor(model => model.Fnac,"Fecha Nacimiento")
        @Html.EditorFor(model => model.Fnac,"date")
        @Html.ValidationMessageFor(model => model.Fnac)

        @Html.LabelFor(model => model.Fono,"Telefono Fijo")
        @Html.EditorFor(model => model.Fono, "tel")
        @Html.ValidationMessageFor(model => model.Fono)

        @Html.LabelFor(model => model.Celular,"Celular")
        @Html.EditorFor(model => model.Celular,"tel")
        @Html.ValidationMessageFor(model => model.Celular)

        @Html.LabelFor(model => model.Mail,"Email")
        @Html.EditorFor(model => model.Mail,"email")
        @Html.ValidationMessageFor(model => model.Mail)

        
        <input type="submit" value="Enviar" />
    </div>
    
    
}
